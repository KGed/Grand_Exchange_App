{
  "name": "Kegs-Grand-Exchange-App",
  "version": "1.0.0",
  "description": "Front end for Keg's Grand Exchange App.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/KGed/Grand_Exchange_App"
  },
  "homepage": "https://github.com/KGed/Grand_Exchange_App",
  "scripts": {
    "dev": "webpack-dev-server --hot --inline",
    "build": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "format": "fuzzy_format(){ loc=$(find . -name $1); echo $loc; prettier-standard  echo $loc; }; fuzzy_format",
    "format-all": "prettier-standard 'src/**/*.js'"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier-standard",
        "git add"
      ]
    },
    "ignore": [
      "**/build/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-1": "^6.1.18",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "lodash": "^4.17.11",
    "node-fetch": "^2.5.0",
    "prettier-standard": "^9.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-promise": "^3.0.2",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "styled-components": "^4.2.0"
  }
}
